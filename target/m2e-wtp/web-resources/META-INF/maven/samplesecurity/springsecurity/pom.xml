  <groupId>samplesecurity</groupId>
  <artifactId>springsecurity</artifactId>
  <packaging>war</packaging>
  <version>0.0.1-SNAPSHOT</version>
  <name>springsecurity Maven Webapp</name>
  <description>Sample spring security web application</description>
  <url>http://maven.apache.org</url>
  
  <properties>
    <spring.version>3.0.0.RELESE</spring.version>
    <spring.security.version>2.0.5.RELESE</spring.security.version> 
